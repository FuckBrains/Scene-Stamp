  {
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["declarativeContent","storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo-chrome.png"
      }
    },
    "icons": {
      "16": "logo-chrome.png"
    },
    "manifest_version": 2
  }